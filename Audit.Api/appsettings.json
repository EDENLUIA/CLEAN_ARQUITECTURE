{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/webapi-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3} {Username} {Message:lj}{Exception}{NewLine}"
        }
      }
    ]
  },
  "CadenaDocker": "Server=host.docker.internal,1433;Database=AuditDB;User Id=sa;Password=fisuncp;TrustServerCertificate=True;",
  "ConnectionStrings": {
    "MyAppDB": "Server=localhost;Database=AuditDB;User Id=sa;Password=fisuncp;TrustServerCertificate=True;"
  },
  "kafka": {
    "Host": "localhost:29092",
    "SocketTimeoutMs": "5000",
    "MessageTimeoutMs": "5000"

  },
  "AllowedHosts": "*"
}